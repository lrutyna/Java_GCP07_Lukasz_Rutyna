<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChatController">
   <children>
      <TextArea fx:id="textArea" layoutX="24.0" layoutY="26.0" prefHeight="268.0" prefWidth="441.0" />
      <TextField fx:id="textField" layoutX="24.0" layoutY="322.0" onAction="#onEnter" prefHeight="25.0" prefWidth="441.0" />
      <Button fx:id="logoutButton" layoutX="477.0" layoutY="269.0" mnemonicParsing="false" onAction="#onLogout" prefHeight="25.0" prefWidth="105.0" text="Wyloguj" />
      <Button fx:id="connectButton" layoutX="477.0" layoutY="200.0" mnemonicParsing="false" onAction="#onConnect" prefHeight="25.0" prefWidth="105.0" text="Połącz" />
      <Button fx:id="disconnButton" layoutX="477.0" layoutY="236.0" mnemonicParsing="false" onAction="#onDisconn" prefHeight="25.0" prefWidth="105.0" text="Rozłącz" />
   </children>
</AnchorPane>
